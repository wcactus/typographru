{
    "name": "wcactus/typographru",
    "description": "Laravel 5 screen typography module for russian language",
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Folumenov",
            "email": "anton.folumenov@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Wcactus\\TypographRu\\": "src/"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "Wcactus\\TypographRu\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wcactus\\TypographRu\\ServiceProvider"
            ],
            "aliases": {
                "TypographRu": "Wcactus\\TypographRu\\Facade"
            }
        }
    }
}
